{
  "extends": [
    "config:base"
  ],
  "rangeStrategy": "replace",
  "lockFileMaintenance": {
    "enabled": true,
    "extends": "schedule:weekly"
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "Dockerfile"
      ],
      "matchStrings": [
        "ENV RCLONE_VERSION=(?<currentValue>.*?)\\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "rclone/rclone"
    }
  ]
}
